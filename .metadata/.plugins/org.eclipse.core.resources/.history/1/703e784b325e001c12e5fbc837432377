package game.interaction;

public class Main03 {

	public static void main(String[] args) {
		Magicain m1 = new Magicain("럭스");
		
		Archer a1 = new Archer("애쉬");
		
		Orc o1 = new Orc();
		
		m1.huntOrc(o1);
		m1.huntOrc(o1);
		
		
		Troll t1 = new Troll();
		
		m1.huntTroll(t1); // 마법사가 한대
		a1.huntTroll(t1); // 궁수가 한대

	}
}
