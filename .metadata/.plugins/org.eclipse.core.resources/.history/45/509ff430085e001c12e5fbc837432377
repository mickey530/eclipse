package game.interaction;

public class Orc {
	private int hp;
	private int atk;
	private int def;
	
	public Orc() {
		this.hp = 5;
		this.atk = 3;
		this.def = 0;
	}

	// getter / setter를 Warrior, Orc에 만들어주세요

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}

	public int getAtk() {
		return atk;
	}

	public void setAtk(int atk) {
		this.atk = atk;
	}

	public int getDef() {
		return def;
	}

	public void setDef(int def) {
		this.def = def;
	}
	
}
