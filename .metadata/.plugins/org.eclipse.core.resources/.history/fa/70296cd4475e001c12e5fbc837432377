package interaction;

public class Main01 {

	public static void main(String[] args) {
		
		Seller s1 = new Seller(30);
		
		Seller s2 = new Seller(20)
		
		Buyer b1 = new Buyer(10000);
		
		b1.buyMango(s1, 5);
		b1.buyMango(s1, 3);
		b1.buyMango(s1, 2);
		
		s1.showSeller();
		s2.showSeller();
		b1.showBuyer();
	}
	
	
}
   