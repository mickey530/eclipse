package heapmemory;

public class StringComparison {

	public static void main(String[] args) {
		// 문자열은 같은 문자로 저장시 같은 주소를 공유한다.
		String str1 = "Hello";
		String str2 = "Hello";
		String str3 = "Hallo"
		
		// 일반 변수의 배정 값 조회시 .hashCode() 를 사용.
		System.out.println(str1.hashCode());
		System.out.println(str2.hashCode());
		
		// 문자열은 일부분만 변경이 불가능함.
		// str[0] = "Q";
		
	}
}
