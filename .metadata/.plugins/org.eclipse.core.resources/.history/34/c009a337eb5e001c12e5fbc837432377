package polymorphism;

public class Coder extends Person{
	
	private String lan;

	public Coder(String name, int age, String lan) {
		super(name, age);
		System.out.println("이름 : " + name + ", 나이 : " + age + ", 언어 : " + this.lan);
	}
	public void showPerson() {
		super.showPerson();
		System.out.println("언어 : " + this.lan);

	}
}
 