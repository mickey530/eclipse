package accessmodifier;

public class Warrior {

	// hp, mp, lv, atk, def, exp, name;
	
	public String name;
	public int hp;
	public int mp;
	public int lv;
	public int atk;
	public int def;
	public int exp;
	
	// 생성자를 만들어주시되, name만 사용자에게 입력받고, 나머지는
	// 체력20, 마나10, 레벨1, 공격력3, 방어력0, 경험치0 이 대입되도록 처리해주세요.
	
	
	public Warrior(String n){
		name = n;
		hp = 20;
		mp = 10;
		lv = 1;
		atk = 3;
		def = 0;
		exp = 0;
	}
}
