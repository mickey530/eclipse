package game.interaction;

public class Warrior {

	private String id;
	private int hp;
	private int mp;
	private int atk;
	private int def;
	private int exp;
	
	public Warrior(String id) {
		this.id = id;
		this.hp = 20;
		this.mp = 10;
		this.atk = 4;
		this.def = 1;
		this.exp = 0;
		System.out.println("전사가 생성되었습니다.");
	}
}
