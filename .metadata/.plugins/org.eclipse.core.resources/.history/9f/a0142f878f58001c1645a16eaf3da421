package structure;

public class Structure02P190 {

	public static void main(String[] args) {
		// 사람을 두명 만들어보세요 (a, b)
		
		Person a = new Person();
		a.name = "나는a";
		a.age = 20;
		a.pNum = "01021392477";
		a.uNum = 0;
				
		Person b = new Person();
		b.name = "나는b";
		b.age = 21;
		b.pNum = "01021392477-2";
		b.uNum = 1;
	}
}
